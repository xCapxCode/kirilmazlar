{
  "name": "kirilmazlar-panel",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "K覺r覺lmazlar Panel System",
  "author": "K覺r覺lmazlar Team",
  "scripts": {
    "build:auto-task": "cd packages/auto-task-progression && npm run build",
    "dev": "vite --mode development --port 5000 --strictPort",
    "dev:staging": "vite --mode staging --port 5000 --strictPort",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:dev": "vite build --mode development",
    "analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "serve": "vite preview --port 5000 --strictPort",
    "serve:staging": "vite preview --port 5000 --strictPort",
    "preview": "serve -s dist -l 5000",
    "start": "node server.js",
    "start:frontend": "serve -s dist -l 5000",
    "dev:server": "nodemon server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:production": "node scripts/production-test.cjs",
    "test:build": "npm run build && node scripts/production-test.cjs",
    "test:security": "npm audit && node scripts/production-test.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prod": "NODE_ENV=production eslint .",
    "env:check": "node scripts/check-environment.js",
    "env:validate": "node scripts/validate-environment.cjs",
    "env:generate": "node scripts/validate-environment.cjs --generate",
    "env:summary": "node scripts/validate-environment.cjs --summary",
    "railway:deploy": "node scripts/railway-deploy.cjs",
    "railway:login": "railway login",
    "railway:status": "railway status",
    "railway:logs": "railway logs",
    "railway:domain": "railway domain",
    "railway:variables": "railway variables",
    "security:port-check": "powershell -ExecutionPolicy Bypass -File scripts/port-security-check.ps1",
    "security:port-watch": "powershell -ExecutionPolicy Bypass -File scripts/port-security-check.ps1 -watch",
    "docker:build": "node scripts/docker-manager.js build",
    "docker:build:dev": "node scripts/docker-manager.js build:dev",
    "docker:run": "node scripts/docker-manager.js run",
    "docker:run:dev": "node scripts/docker-manager.js run:dev",
    "docker:logs": "node scripts/docker-manager.js logs",
    "docker:status": "node scripts/docker-manager.js status",
    "docker:cleanup": "node scripts/docker-manager.js cleanup",
    "docker:up": "node scripts/docker-manager.js up",
    "docker:up:dev": "node scripts/docker-manager.js up:dev",
    "docker:down": "node scripts/docker-manager.js down"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "serve": "^14.2.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "typescript": "^5.6.3",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.1",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
