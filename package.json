    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5",
    "vitest": "^0.33.0"
  }
